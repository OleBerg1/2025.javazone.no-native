{
  "expo": {
    "name": "JavaZone2025",
    "slug": "javazone2025",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/javaZone-logo.png",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#e8dacfe6",
    "scheme": "javazone2025",
    "splash": {
      "image": "./assets/images/logo/javaZone-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#e8dacfe6"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "JavaBin",
      "backgroundColor": "#e8dacfe6"
    },
    "android": {
      "package": "no.javabin.javazone2025",
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/javaZone-logo.png",
        "backgroundColor": "#e8dacfe6"
      },
      "splash": {
        "resizeMode": "native"
      }
    },
    "web": {
      "favicon": "./assets/images/logo/javaZone2025-favIcon.png",
      "bundler": "metro",
      "backgroundColor": "#e8dacfe6"
    },
    "extra": {
      "eas": {
        "projectId": "091cabe3-44ce-4d6d-a9d6-eb0e771edc64"
      }
    },
    "newArchEnabled": true,
    "owner": "javazoneapp",
    "plugins": ["expo-localization", "expo-router", "expo-font"]
  }
}
