{
  "expo": {
    "name": "JavaZone2025",
    "slug": "javazone2025",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-sharp.png",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#232526",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo-sharp.png",
      "resizeMode": "contain",
      "backgroundColor": "#232526"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "JavaBin",
      "backgroundColor": "#232526"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-sharp.png",
        "backgroundColor": "#232526"
      },
      "package": "com.JavaBin"
    },
    "web": {
      "favicon": "./assets/images/logo-sharp.png",
      "bundler": "metro",
      "backgroundColor": "#232526"
    },
    "extra": {
      "eas": {
        "projectId": "091cabe3-44ce-4d6d-a9d6-eb0e771edc64"
      }
    },
    "owner": "carolinekristin",
    "plugins": [
      "expo-localization",
      "expo-router"
    ]
  }
}
